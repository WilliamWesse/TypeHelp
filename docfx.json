{
    "build": {
        "content": [
            {
                "files": [
                    "api/**.yml",
                    "api/index.md"
                ]
            },
            {
                "files": [
                    "articles/**.md",
                    "articles/**/toc.yml",
                    "toc.yml",
                    "*.md"
                ]
            }
        ],
        "disableGitFeatures": false,
        "fileMetadataFiles": [],
        "globalMetadataFiles": [
            "TypeHelpMeta.json"
        ],
        "keepFileLink": false,
        "output": "site",
        "postProcessors": [],
        "resource": [
            {
                "files": [
                    "images/**"
                ]
            }
        ],
        "sitemap": {
            "baseUrl": "https://github.com/WilliamWesse/TypeHelp",
            "changefreq": "daily",
            "priority": 0.1
        },
        "template": [
            "templates/default",
            "templates/enumerations"
        ]
    },
    "metadata": [
        {
            "allowCompilationErrors": false,
            "dest": "api",
            "disableDefaultFilter": false,
            "disableGitFeatures": false,
            "includePrivateMembers": false,
            "memberLayout": "SeparatePages",
            "namespaceLayout": "nested",
            "noRestore": false,
            "src": [
                {
                    "exclude": [
                        "README.md"
                    ],
                    "files": [
                        "**.csproj"
                        //, "bin/**/*.dll"
                    ]
                }
            ]
        }
    ]
}
